node n1 {
    type router
    model host
    network-config {
	hostname n1
	!
	interface eth0
	 ip address 10.0.0.10/24
	 ipv6 address 2001:0::10/64
	!
    }
    canvas c1
    iconcoords {162.0 117.0}
    labelcoords {162.0 149.0}
    interface-peer {eth0 n4}
}

